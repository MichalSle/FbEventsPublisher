@model List<FbEvent>
@using EventsProvider;

@{
    ViewBag.Title = "Title some";
    Layout = "../Shared/_Layout.cshtml";
}
<head>
    <title>Bootstrap Case</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js"></script>
</head>


<nav class="navbar navbar-default navbar-fixed-top">
    <div class="container">
        <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                <span class="sr-only">Toggle navigation</span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#"></a>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
                <li class="active">
                    <a href="#">Home</a>
                </li>
                <li>
                    <a href="#about">About</a>
                </li>

                <li>
                    <a href="#contact">IT</a>
                </li>
                <li>
                    <a href="#contact">Impreza</a>
                </li>
                <li>
                    <a href="#contact">Sport</a>
                </li>
                <li>
                    <a href="#contact">Kulinaria</a>
                </li>
            </ul>
        </div>
    </div>
</nav>

<div class="container">
    @for (int i = 0; i < Model.Count; i++)
    {
        <div style="margin-bottom: 10px;">
            <div>
                @Html.LabelFor(m => m[i].Name)
                @Html.DisplayFor(m => m[i].Name)
            </div>
            <div>
                @Html.LabelFor(m => m[i].Description)
                @Html.DisplayFor(m => m[i].Description)
            </div>
            <div>
                @Html.LabelFor(m => m[i].Location)
                @Html.DisplayFor(m => m[i].Location)
            </div>
            <div>
                @Html.LabelFor(m => m[i].Owner)
                @Html.DisplayFor(m => m[i].Owner)
            </div>
            <div>
                @Html.LabelFor(m => m[i].StartDate)
                @Html.DisplayFor(m => m[i].StartDate)
            </div>
        </div>
    }
</div>